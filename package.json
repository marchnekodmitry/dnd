{
    "name": "react-boilerplate",
    "version": "0.0.0",
    "author": "",
    "private": true,
    "scripts": {
        "start": "cross-env-shell NODE_ENV=development node --require @babel/register \"./scripts/webpack/start.js\"",
        "build": "cross-env-shell NODE_ENV=production node --require @babel/register \"./scripts/webpack/build.js\"",
        "build:prod:debug": "cross-env-shell NODE_ENV=production node --inspect-brk --require @babel/register \"./scripts/webpack/build.js\"",
        "analyze": "webpack-bundle-analyzer \"./build/stats.json\"",
        "build:analyze": "cross-env-shell ANALYZE=true run-s build analyze",
        "serve": "serve ./build --single",
        "lint": "eslint src/**/*.{ts,tsx}",
        "lint:fix": "npm run lint --fix"
    },
    "dependencies": {
        "@types/react-helmet": "^6.0.0",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0",
        "styled-components": "^5.1.1",
        "styled-normalize": "^8.0.7"
    },
    "devDependencies": {
        "@babel/core": "7.1.2",
        "@babel/plugin-proposal-class-properties": "7.1.0",
        "@babel/plugin-syntax-async-generators": "7.0.0",
        "@babel/plugin-syntax-dynamic-import": "7.0.0",
        "@babel/preset-env": "7.1.0",
        "@babel/preset-react": "7.0.0",
        "@babel/preset-typescript": "^7.10.4",
        "@babel/register": "7.0.0",
        "@hot-loader/react-dom": "^16.13.0",
        "@svgr/webpack": "^5.4.0",
        "@types/react": "^16.9.43",
        "@types/react-dom": "^16.9.8",
        "@types/react-router-dom": "^5.1.5",
        "@types/styled-components": "^5.1.1",
        "@types/webpack": "^4.41.21",
        "@typescript-eslint/eslint-plugin": "^3.6.1",
        "@typescript-eslint/parser": "^3.6.1",
        "app-root-path": "2.1.0",
        "babel-eslint": "10.0.1",
        "babel-loader": "8.0.4",
        "babel-plugin-dynamic-import-node": "2.2.0",
        "chalk": "2.4.1",
        "clean-webpack-plugin": "0.1.19",
        "copy-webpack-plugin": "^6.0.3",
        "cross-env": "5.2.0",
        "css-loader": "1.0.1",
        "eslint": "^7.4.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-react": "^1.1.7",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.3",
        "file-loader": "2.0.0",
        "fork-ts-checker-webpack-plugin": "^5.0.7",
        "friendly-errors-webpack-plugin": "1.7.0",
        "html-webpack-plugin": "3.2.0",
        "husky": "^4.2.5",
        "imagemin-mozjpeg": "8.0.0",
        "imagemin-pngquant": "6.0.0",
        "imagemin-svgo": "7.0.0",
        "imagemin-webpack": "^5.1.1",
        "lint-staged": "^10.2.11",
        "mini-css-extract-plugin": "0.4.4",
        "npm-run-all": "4.1.3",
        "pre-commit": "^1.2.2",
        "precise-commits": "^1.0.2",
        "prettier": "^2.0.5",
        "react-dev-utils": "6.1.1",
        "react-helmet": "^6.1.0",
        "react-hot-loader": "^4.12.21",
        "serve": "^11.3.2",
        "style-loader": "0.23.1",
        "terser-webpack-plugin": "^3.0.7",
        "tsconfig-paths-webpack-plugin": "^3.2.0",
        "typescript": "^3.9.7",
        "webpack": "^4.43.0",
        "webpack-bundle-analyzer": "^3.8.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "webpack-dev-server-waitpage": "1.0.0",
        "webpack-hot-middleware": "2.24.3",
        "webpack-merge": "4.1.4"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*": [
            "precise-commits",
            "git add ."
        ],
        "*.ts|*.tsx": [
            "npm run lint:fix"
        ]
    }
}
